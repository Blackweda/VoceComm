                if(MARKETWATCH_SCREENER){


                    // BIOTECHNOLOGY
                    Document doc = Jsoup.connect(AUTOMOTIVE).get();
                    int totalCount = 0;
                    EnvironmentDiagnostics("TotalCount is: " + totalCount);

                    Elements divs = doc.getElementsByTag("div");

                    for(Element div : divs){
                        if(/*div.className().contains("floatleft") &&*/ div.className().contains("results")){
                            String queryCounter = div.text();
                            EnvironmentDiagnostics("querycountertext is: " + queryCounter);
                            String[] totalValue = queryCounter.split("of");
                            String numberOnly= totalValue[1].replaceAll("[^0-9]", "");
                            totalCount = Integer.parseInt(numberOnly);
                            break;
                        }
                    }
                    EnvironmentDiagnostics("TotalCount is: " + totalCount);
                    if(totalCount > 1200)
                        Toast.makeText(getApplicationContext(), "More than 1200 Stocks!!", Toast.LENGTH_LONG).show();
                    maintextviewStr += "\n\n Total Automotive Stocks: " + totalCount + "\n\n";

                    if(totalCount > 100){
                        doc = Jsoup.connect(AUTOMOTIVE_101_200).get();

                        Elements tables = doc.getElementsByTag("td");
                        for (Element td : tables) {

                            if (td.className().contains("aleft") && !td.className().contains("company-col"))
                                market_watch_list.add(td.text());
                        }
                        EnvironmentDiagnostics("TC>100: " + market_watch_list.size());
                    }
                    if(totalCount > 0){
                        doc = Jsoup.connect(AUTOMOTIVE).get();

